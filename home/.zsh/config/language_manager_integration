# vim: set ft=sh:

# async init of nvm
export NVM_DIR="$HOME/.nvm"

# Mac
[ -e "/usr/local/opt/nvm/nvm.sh" ] && source /usr/local/opt/nvm/nvm.sh --no-use

# Linux
[ -s "${NVM_DIR}/nvm.sh" ] && source ${NVM_DIR}/nvm.sh --no-use

# boot rbenv
if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi

